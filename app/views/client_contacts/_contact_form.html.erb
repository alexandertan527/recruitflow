<%= simple_form_for(@contact) do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :client %>
  <%= f.input :position %>
  <%= f.input :status, collection: [ "Active", "Hiring", "Inactive" ], as: :select %>
  <%= f.input :phone_number %>
  <%= f.input :email %>
  <%= f.submit "Save List", class: "btn btn-primary" %>
<% end %>
